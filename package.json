{
	"name": "eden-tanstack-query",
	"private": true,
	"type": "module",
	"packageManager": "bun@1.3.6",
	"license": "Apache-2.0",
	"author": "Ilya Zhidkov",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xkelxmc/eden-tanstack-query.git"
	},
	"homepage": "https://github.com/xkelxmc/eden-tanstack-query#readme",
	"workspaces": [
		"packages/*",
		"examples/*"
	],
	"scripts": {
		"postinstall": "bun run --filter='@examples/*' db:generate",
		"dev": "bun run --filter=* dev",
		"build": "bun run --filter=eden-tanstack-react-query build",
		"postbuild": "bun run scripts/update-sizes.ts",
		"size": "bun run scripts/update-sizes.ts",
		"example:basic": "bun run --filter=@eden-tanstack-query/example-basic dev",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:cov": "vitest run --coverage",
		"test:types": "bunx tsc --noEmit",
		"test:all": "vitest run && bunx tsc --noEmit",
		"lint": "bunx biome check .",
		"lint:fix": "bunx biome check --write .",
		"format": "bunx biome format --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@types/bun": "latest",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"happy-dom": "20.3.7",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	}
}
